<mvc:View controllerName="excelupload.controller.ExcelUpload"
   xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core" xmlns="sap.m" class="sapUiSizeCompact">
    <Page id="page" title="{i18n>title}" titleAlignment="Center">
        <content>
            <core:Fragment fragmentName="excelupload.fragments.commonUploadUI" type="XML" />
        </content>  
        <footer>
            <OverflowToolbar id="idOFT">
				<ToolbarSpacer id="idTBS"/>
                <Button id="idSubmitBtn" text="{i18n>submit}" press="handlePress"/>
            </OverflowToolbar>
        </footer>      
    </Page>
</mvc:View>
